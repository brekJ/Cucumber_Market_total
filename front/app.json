{
  "expo": {
    "name": "오이마켓",
    "slug": "Cucumber_Market",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cucumber_icon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/cucumber_splash.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "test"
    },
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.sunwonga.Cucumber_Market"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app needs access your camera to take photos"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "cbb81270-9090-45e8-8b88-6908464f77a3"
      }
    }
  }
}
